const playVideo = async () => {
    if(video.current){
      await video.current.playAsync()
      setIsPlaying(true);
    }
}

  const pauseVideo = async () => {
    if(video.current){
      await video.current.pauseAsync()
      setIsPlaying(false);
    }
}

  const handleSkipBackward = async () => {
    if (video.current) {
      const status = await video.current.getStatusAsync();
      const newPosition = status.positionMillis - 10000; // Skip 10 seconds backward
      video.current.setPositionAsync(newPosition < 0 ? 0 : newPosition);
    }
};

  const handleSkipForward = async () => {
    if (video.current) {
      const status = await video.current.getStatusAsync();
      const duration = status.durationMillis;
      const newPosition = status.positionMillis + 10000; // Skip 10 seconds forward
      video.current.setPositionAsync(newPosition > duration ? duration : newPosition);
    }
}
